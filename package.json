{
  "name": "packages",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "echo test",
    "clean": "pnpm -r exec -- rm -rf node_modules",
    "clean-build": "pnpm -r exec -- rm -rf dist",
    "dev": "pnpm -r dev",
    "p-build": "pnpm -r build",
    "prepare": "husky install",
    "build": "turbo run build ",
    "t-graph": "turbo run build --graph",
    "deploy": "gh-pages -d packages/client/dist ",
    "serve": "pnpm  serve --filter=client"
  },
  "author": "",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "husky": "^7.0.2",
    "turbo": "^1.0.11"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  },
  "dependencies": {
    "gh-pages": "^3.2.3"
  }
}
